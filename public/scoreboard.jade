  body
    #wrapper
      // Navigation
      nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='index.html') SB Admin
        // Top Menu Items
        ul.nav.navbar-right.top-nav
          li.dropdown
          li.dropdown
            a.dropdown-toggle(href='#', data-toggle='dropdown')
              i.fa.fa-user
              |  John Smith
              b.caret
            ul.dropdown-menu
              li
                a(href='#')
                  i.fa.fa-fw.fa-user
                  |  Profile
              li
                a(href='#')
                  i.fa.fa-fw.fa-envelope
                  |  Inbox
              li
                a(href='#')
                  i.fa.fa-fw.fa-gear
                  |  Settings
              li.divider
              li
                a(href='index.html')
                  i.fa.fa-fw.fa-power-off
                  |  Log Out
        // Sidebar Menu Items - These collapse to the responsive navigation menu on small screens
        .collapse.navbar-collapse.navbar-ex1-collapse
          ul.nav.navbar-nav.side-nav
            li
              a(href='admin.html')
                i.fa.fa-fw.fa-dashboard
                |  Dashboard
            li
              a(href='questionadmin.html')
                i.fa.fa-fw.fa-bar-chart-o
                |  Question
            li.active
              a(href='scoreboard.html')
                i.fa.fa-fw.fa-table
                |  Score Board
            li
            
        // /.navbar-collapse
      #page-wrapper
        .container-fluid
          // Page Heading
          .row
            .col-lg-12
              h1.page-header
                | Score Board
              ol.breadcrumb
                li
                  i.fa.fa-dashboard
                  a(href='admin.html') Dashboard
                li.active
                  i.fa.fa-table
                  |  Score Board
          // /.row
          .row
            .col-lg-12
              center
                h2 Score Board
              .table-responsive
                table.table.table-bordered
                  thead
                    tr
                      th User
                      th Score
                  tbody
                    tr
                      td /index.html
