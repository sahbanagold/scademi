  body#page-top.index
    // Navigation
    nav#mainNav.navbar.navbar-default.navbar-fixed-top.navbar-custom-exam
      .container
        // Brand and toggle get grouped for better mobile display
        .navbar-header.page-scroll
          button.navbar-toggle(type='button', data-toggle='collapse')
            span.sr-only Toggle navigation
            |  Menu
            i.fa.fa-bars
          img#arrow_left.navbar-brand(src='img/glyphicons-211-arrow-left.png', alt='')
          img#arrow_right.navbar-brand(src='img/glyphicons-212-arrow-right.png', alt='')
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
          ul.nav.navbar-nav.navbar-right
            li.hidden
              a(href='#page-top')
            li
              a(href='#about') Time
            li
              p(style='line-height: 50px;')
                button#btn_hour.btn.btn-default.btn_time(type='button') 01
                button#btn_minute.btn.btn-default.btn_time(type='button') 02
                button#btn_second.btn.btn-default.btn_time(type='button') 02
            li
              a(href='#about') Current Question
            li
            li
              p(style='line-height: 50px; text-align: center;')
                button#current.btn.btn-default(type='button') 10
            li
              a(href='#about') Answer
            li
              p(style='line-height: 50px; text-align: center;')
                button#btn_qanswer.btn.btn-default(type='button') 11
            li
              a(href='#about')
            li
              p(style='line-height: 50px; text-align: center;')
                button#btn_no1.btn.btn-default(type='button') 1
                button#btn_no2.btn.btn-default(type='button') 2
                button#btn_no3.btn.btn-default(type='button') 3
                button#btn_no4.btn.btn-default(type='button') 4
                button#btn_no5.btn.btn-default(type='button') 5
                button#btn_no6.btn.btn-default(type='button') 6
                button#btn_no7.btn.btn-default(type='button') 7
                button#btn_no8.btn.btn-default(type='button') 8
                button#btn_no9.btn.btn-default(type='button') 9
                button#btn_no10.btn.btn-default(type='button') 10
            li
              p(style='line-height: 50px; text-align: center;')
                img#arrow_right.navbar-brand(src='img/glyphicons-214-arrow-up.png', alt='')
                img#arrow_right.navbar-brand(src='img/glyphicons-213-arrow-down.png', alt='')
                a(href='/score')
                  button#btn_finish.btn-success.btn-lg(type='button') Finish
        // /.navbar-collapse
      // /.container-fluid
    // Header
    header
      .container
        h1 Pertanyaan
        .row
          .col-lg-12
            .row
              .col-lg-6.text-center
                .panel.panel-default(class='rows=', cols='50%')
                  .panel-body.question
                    p#question
                      | question
              .col-lg-6(style='text-align: left;')
                div
                  label.radio-inline
                    input#option1(type='radio', name='optradio')
                    | A
                div
                  label.radio-inline
                    input#option2(type='radio', name='optradio')
                    | B
                div
                  label.radio-inline
                    input#option3(type='radio', name='optradio')
                    | C
                div
                  label.radio-inline
                    input#option4(type='radio', name='optradio')
                    | D
                div
                  label.radio-inline
                    input#option5(type='radio', name='optradio')
                    | E
            .intro-text
              br
              button#btn_answer.btn-success.btn-lg.colorbtn(type='submit') Answer
    // Footer
    footer.text-center
      .footer-above
        .container
          .row
            .footer-col.col-md-4
